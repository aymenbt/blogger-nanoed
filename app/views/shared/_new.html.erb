
<% @nantes.each do |nante| %>

<div class="row">

  

    <a href="#">
    

      <article class="article-row">
        <div class="article-row-img", size="50x50">
          <%= image_tag(nante.image, width:"193px", height:"130px", alt: "picture of a whale eating a donkey") %>
        </div>
        
        <div class="article-row-content">
          <h1 class="article-row-content-header"><%= nante.title %></h1>

          <p class="article-row-content-description"><%= nante.description %></p>

          <p class="article-row-content-author">By Yeti</p>
          <time class="article-row-content-time" datetime="2008-02-14 20:00"><%= nante.published_at %></time>
        </div>
      </article>
    </a>
</div>


    <% end %>